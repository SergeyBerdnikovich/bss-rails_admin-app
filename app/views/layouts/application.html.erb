<!DOCTYPE html>
<html>
<head>
  <title>TestApp</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="header">
  <%= render "layouts/links" %>
</div>
<% flash.each do |name, msg| %>
	<%= content_tag :div, raw(msg), :id => "flash_#{name}" %>
<% end %>
<%= yield %>

</body>
</html>
